<%- include("partials/header") -%>

    <h1>Home</h1>
    <p>
        <%= homecontent %>
    </p>

    <div class="posts">
        <% posts.forEach(function(post){ %>
            <h3>
                <%= post.title %>
            </h3>

            <p>
                <%= post.postcontent.substring(0,100)+"..." %>
                <a href="/posts/<%= post.title %>">Read More</a>
            </p>
        <% }) %>
        <br>
    </div>

    <%-include("partials/footer")-%>